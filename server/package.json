{
    "name": "performance-matters-server",
    "private": true,
    "author": "Maikel van Veen <maikeloo10@hotmail.com>",
    "license": "MIT",
    "scripts": {
        "compile": "tsc",
        "open-chrome": "wait-on http://localhost:1234 && open -a \"Google Chrome\" http://localhost:1234",
        "start-production": "tsc && node ./dist/index.js",
        "start-server": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
        "watch-server": "concurrently \"yarn start-server\" \"yarn open-chrome\" --raw"
    },
    "devDependencies": {
        "@types/express": "^4.16.1",
        "@types/helmet": "^0.0.43",
        "concurrently": "^4.1.0",
        "ts-node-dev": "^1.0.0-pre.32",
        "typescript": "^3.3.3333",
        "wait-on": "^3.2.0",
        "yarn": "^1.13.0"
    },
    "dependencies": {
        "express": "^4.16.4",
        "helmet": "^3.16.0"
    }
}
