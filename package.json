{
    "name": "performance-matters",
    "private": true,
    "author": "Maikel van Veen <maikeloo10@hotmail.com>",
    "license": "MIT",
    "scripts": {
        "build": "tsc && gulp minifyCss && copyfiles -E -u 2 \"./server/src/views/**/*\" ./server/dist",
        "open-chrome": "wait-on http://localhost:3000 && open -a \"Google Chrome\" http://localhost:3000",
        "start": "node server/dist/index.js",
        "start-server": "ts-node-dev --respawn --transpileOnly ./server/src/index.ts",
        "watch-server": "concurrently \"gulp minifyCss\" \"yarn start-server\" \"yarn open-chrome\" --raw"
    },
    "devDependencies": {
        "@types/body-parser": "^1.17.0",
        "@types/compression": "^0.0.36",
        "@types/ejs": "^2.6.3",
        "@types/express": "^4.16.1",
        "@types/helmet": "^0.0.43",
        "@types/lodash.flatten": "^4.4.6",
        "@types/memory-cache": "^0.2.0",
        "@types/node-fetch": "^2.1.6",
        "concurrently": "^4.1.0",
        "copyfiles": "^2.1.0",
        "gulp": "^4.0.0",
        "gulp-autoprefixer": "^6.0.0",
        "gulp-clean-css": "^4.0.0",
        "ts-node-dev": "^1.0.0-pre.32",
        "tslint": "^5.13.1",
        "tslint-eslint-rules": "^5.4.0",
        "typescript": "^3.3.3333",
        "wait-on": "^3.2.0",
        "yarn": "^1.13.0"
    },
    "dependencies": {
        "body-parser": "^1.18.3",
        "clean-deep": "^3.0.2",
        "compression": "^1.7.3",
        "ejs": "^2.6.1",
        "express": "^4.16.4",
        "helmet": "^3.16.0",
        "lodash.flatten": "^4.4.0",
        "memory-cache": "^0.2.0",
        "node-fetch": "^2.3.0"
    },
    "engineStrict": true,
    "engines": {
        "node": "8.11.0"
    }
}
